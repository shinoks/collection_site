<script>
    $(function() {
        if( document.cookie.indexOf( "runOnce" ) < 0 ) {
            $( "#myModal" ).fadeIn();
            $( "#myModal" ).addClass('in');
        }
        $("#modal_button_cookies").click(function(){
                $( "#myModal" ).removeClass('in');
                $( "#myModal" ).fadeOut();
                document.cookie = "runOnce=true; expires=Fri, 31 Dec 9999 23:59:59 GMT; path=/";
            }
        );
    })
</script>
