    <script>
        $(document).ready(function(){
            $(window).scroll(function(){
                if ($(this).scrollTop() > 30) {
                    $('#navi').addClass('fixed_menu');
                    $('#navi2').removeClass('hidden');
                } else {
                    $('#navi').removeClass('fixed_menu');
                    $('#navi2').addClass('hidden');
                }
            });

        });
        $(window).load(function(){
            if ($(window).scrollTop() > 30) {
                $('#navi').addClass('fixed_menu');
                $('#navi2').removeClass('hidden');
            }});
    </script>
