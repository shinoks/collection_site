{% include 'front/addons/header.html.twig' %}

{% set menus = menu.getMenu %}
{% set activeMenu = menu.getActiveMenu %}
{% for module in menu.modulesForActivePage.top %}
    {% block top %}
        {% set content = module.content %}
        <a href="#" class="anchor"  id="module-{{ module.id }}"></a>
        {%  include 'front/modules/' ~ module.template %}
    {% endblock %}
{% endfor %}

{% block body %}{% endblock %}

{% for module in menu.modulesForActivePage.bottom %}
    {% block bottom %}
        {% set content = module.content %}
        <a href="#" class="anchor" id="module-{{ module.id }}"></a>
        {%  include 'front/modules/' ~ module.template %}
    {% endblock %}
{% endfor %}

{% include 'front/addons/footer.html.twig' %}


